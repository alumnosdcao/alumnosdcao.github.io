---
layout: null
---
[
  {% for post in site.materias-at %}
    {
      "materia"    : "{{ post.title | escape }}",
      "url"      : "{{ site.baseurl }}{{ post.url }}",
      "correlativas": "{% for correlativa in post.correlativas %}
                          {{ correlativa.materia }}, 
                        {% endfor %}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]